msgid "``results$table``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:85
msgid "a table of the proportions and test results"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:86
msgid "``results$postPlots``"
msgstr ""

#: ../../_pages/jmv_propTest2.rst:86
msgid "an array of the posterior plots"
msgstr ""

